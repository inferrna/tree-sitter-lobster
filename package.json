{
  "name": "tree-sitter-lobster",
  "version": "0.21.0",
  "description": "Lobster grammar for tree-sitter",
  "repository": "github:tree-sitter/tree-sitter-lobster",
  "license": "MIT",
  "author": "Max Brunsfeld",
  "contributors": [
    "Amaan Qureshi"
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "lobster"
  ],
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "tree-sitter-cli": "^0.22.6",
    "prebuildify": "^6.0.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tree-sitter generate --no-bindings",
    "lint": "eslint grammar.js",
    "parse": "tree-sitter parse",
    "test": "tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "tree-sitter": [
    {
      "scope": "source.lobster",
      "file-types": [
        "lobster"
      ],
      "injection-regex": "lobster",
      "highlights": "queries/highlights.scm",
      "tags": "queries/tags.scm"
    }
  ]
}
